<html lang="en-US">
<head>
    <title>Survey Analysis</title>
</head>

<body>
    <h1>Survey analysis</h1>
    <h2>Visual Appeal</h2>
    <ul>
    <% for (var i=0; i<results[0].length; i++){ %>
        <li><%= results[0][i].visual %> :  <%= results[0][i].count %></li>
    <% } %>    
    </ul>

    <h2>Ease</h2>
    <ul>
    <% for (var i=0; i<results[1].length; i++){ %>
        <li><%= results[1][i].ease %> :  <%= results[1][i].count %></li>
    <% } %>    
    </ul>

    <h2>Strength</h2>
    <ul>
    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[2][i].strength %> :  <%= results[2][i].count %></li>
    <% } %>    
    </ul>

    <h2>Issues</h2>
    <ul>
    <% for (var i=0; i<results[3].length; i++){ %>
        <li><%= results[3][i].issue %> :  <%= results[3][i].count %></li>
    <% } %>    
    </ul>

    <h2>Comments</h2>
    <ul>
    <% for (var i=0; i<results[4].length; i++){ %>
        <li><%= results[4][i].comments %> :  <%= results[4][i].count %></li>
    <% } %>    
    </ul>
</body>
</html>